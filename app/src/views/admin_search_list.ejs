<%- include('admin_nav.ejs')%>
  <header>
    <h1 class="header__title">관리자 게시판</h1>
  </header>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">글번호</th>
        <th scope="col">제목</th>
        <th scope="col">날짜</th>
        <th scope="col">작성자</th>
      </tr>
    </thead>
    <tbody>
      <% for(var i=0; i < posts.length; i++){ %>
        <tr>
          <th scope="row">
            <%= posts[i]._id %>
          </th>
          <td><a href="/admin/detail/<%= posts[i]._id %>">
              <%= posts[i].제목 %>
            </a></td>
          <td>
            <%= posts[i].작성날짜 %>
          </td>
          <td>군침이싹</td>
        </tr>
        <% } %>
    </tbody>
    <br>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.js"></script>
</body>
</html>